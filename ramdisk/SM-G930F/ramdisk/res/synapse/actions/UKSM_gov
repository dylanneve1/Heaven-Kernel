# Busybox 
if [ -e /su/xbin/busybox ]; then
	BB=/su/xbin/busybox;
else if [ -e /sbin/busybox ]; then
	BB=/sbin/busybox;
else
	BB=/system/xbin/busybox;
fi;
fi;

if [ -f "$1" ]; then
	if [ ! -z "$2" ]; then
		echo "$2" > "$1";
	fi;

	$BB echo "$($BB cat "$1" | $BB awk 'NR>1{print $1}' RS=[ FS=])";
fi;
